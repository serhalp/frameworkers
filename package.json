{
  "name": "frameworkers",
  "description": "Frameworkers: the top contributors to front-end frameworks",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "format": "prettier --write  \"**/*.{json,ts,mjs,cjs,md,astro}\" \"**/.*.{json,ts,mjs,cjs,md,astro}\"",
    "format:check": "prettier --check \"**/*.{json,ts,mjs,cjs,md,astro}\" \"**/.*.{json,ts,mjs,cjs,md,astro}\"",
    "lint": "eslint --fix \"**/*.{ts,mjs,cjs,astro}\" \"**/.*.{ts,mjs,cjs,astro}\"",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "npm run typecheck && npm run lint && npm run format",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/netlify": "^6.6.3",
    "@octokit/rest": "^22.0.1",
    "astro": "^5.16.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.0"
  }
}
